   ],
   "source": [
    "a=0\n",
    "b=1\n",
    "i=0\n",
    "while i<10:\n",
    "    c=a+b\n",
    "    print(c)\n",
    "    a=b\n",
    "    b=c\n",
    "    i+=1"
